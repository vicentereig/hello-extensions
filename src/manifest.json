{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup/hello.html",
    "default_icon": "assets/hello_extension.png"
  },
  "content_scripts": [
    {
      "js": ["sites/github.js"],
      "matches": [
        "https://github.com/*"
      ]
    }
  ]
}
